#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/bin/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/bin/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/bin/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/knex@3.1.0_mysql2@3.12.0_pg@8.13.3_sqlite3@5.1.7_tedious@18.6.1/node_modules/knex/bin/cli.js" "$@"
fi
