#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules/directus/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules/directus/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules:/Users/ryanbrooks/Projects/directus-backend-clean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules/directus/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../directus@11.6.1_@types+node@22.14.0_@unhead+vue@1.11.20_vue@3.5.13__encoding@0.1.13_lod_dde50afe26305ab075936359f64f122a/node_modules/directus/cli.js" "$@"
fi
